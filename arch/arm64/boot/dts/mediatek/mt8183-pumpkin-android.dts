/dts-v1/;
/plugin/;

&firmware {
	android {
		compatible = "android,firmware";
		hardware = "mt8183";
		fstab {
			compatible = "android,fstab";
			vendor {
				compatible = "android,vendor";
				dev = "/dev/block/platform/soc/11230000.mmc/by-name/vendor";
				type = "ext4";
				mnt_flags = "ro";
				fsmgr_flags = "wait";
			};
			system {
				compatible = "android,system";
				dev = "/dev/block/platform/soc/11230000.mmc/by-name/system";
				type = "ext4";
				mnt_flags = "ro";
				fsmgr_flags = "wait";
			};
		};
	};
};

&reserved_memory {
	ramoops@0x4FF00000 {
		compatible = "ramoops";
		reg = <0x0 0x4FF00000 0x0 0x00100000>;
		record-size = <0x8000>;
		console-size = <0x8000>;
		ftrace-size = <0x0>;
		pmsg-size = <0x8000>;
	};
};
